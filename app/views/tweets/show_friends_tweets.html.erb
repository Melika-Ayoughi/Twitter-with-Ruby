

<h1>Listing Friends's Tweets</h1>

<table>
  <thead>
  <tr>
    <th>User</th>
    <th>Text</th>
    <th>Date Created</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @user_tweets.each do |user_tweet| %>
      <tr>
        <td><%= user_tweet[:user].email %></td>
        <td><%= user_tweet[:tweet].text %></td>
        <td><%= user_tweet[:tweet].created_at %></td>
      </tr>
  <% end %>
  </tbody>
</table>


<%= link_to 'My Tweets', tweets_path %>